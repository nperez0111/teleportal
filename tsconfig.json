{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "types": ["@types/bun"],
    "paths": {
      "teleportal/encryption-key": ["./src/encryption-key/index.ts"],
      "teleportal/storage": ["./src/storage/index.ts"],
      "teleportal/transports/redis": ["./src/transports/redis/index.ts"],
      "teleportal/transports/nats": ["./src/transports/nats/index.ts"],
      "teleportal/transports/rate-limiter": [
        "./src/transports/rate-limiter/index.ts"
      ],
      "teleportal/transports": ["./src/transports/index.ts"],
      "teleportal/http": ["./src/http/index.ts"],
      "teleportal/server": ["./src/server/index.ts"],
      "teleportal/protocol/encryption": [
        "./src/lib/protocol/encryption/index.ts"
      ],
      "teleportal/protocol": ["./src/lib/protocol/index.ts"],
      "teleportal/protocols/milestone": ["./src/protocols/milestone/index.ts"],
      "teleportal/protocols/file": ["./src/protocols/file/index.ts"],
      "teleportal/providers": ["./src/providers/index.ts"],
      "teleportal/websocket-server": ["./src/websocket-server/index.ts"],
      "teleportal/monitoring": ["./src/monitoring/index.ts"],
      "teleportal/token": ["./src/token/index.ts"],
      "teleportal/merkle-tree": ["./src/lib/merkle-tree/index.ts"],
      "teleportal/devtools": ["./src/devtools/index.ts"],
      "teleportal": ["./src/lib/index.ts"]
    },
    "jsx": "react-jsx"
  },
  "include": ["src", "playground", "examples", "guides"]
}
