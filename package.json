{
  "name": "teleportal",
  "version": "0.0.4",
  "description": "A storage, transport & runtime agnostic Y.js server/provider. Built on web primitives, supports subdocs, and handles everything without in-memory storage. Perfect for collaborative apps! ðŸš€",
  "keywords": [
    "yjs",
    "server",
    "provider",
    "storage",
    "transport",
    "protocol",
    "e2ee",
    "runtime",
    "subdocs"
  ],
  "repository": "nperez0111/teleportal",
  "license": "MPL-2.0",
  "sideEffects": false,
  "type": "module",
  "workspaces": [
    "playground",
    "examples/*",
    "docs"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/lib/index.mjs",
    "./storage": "./dist/storage/index.mjs",
    "./http": "./dist/http/index.mjs",
    "./protocol": "./dist/lib/protocol/index.mjs",
    "./protocols/milestone": "./dist/protocols/milestone/index.mjs",
    "./protocols/file": "./dist/protocols/file/index.mjs",
    "./protocol/encryption": "./dist/lib/protocol/encryption/index.mjs",
    "./providers": "./dist/providers/index.mjs",
    "./transports/redis": "./dist/transports/redis/index.mjs",
    "./transports/nats": "./dist/transports/nats/index.mjs",
    "./transports/rate-limiter": "./dist/transports/rate-limiter/index.mjs",
    "./transports": "./dist/transports/index.mjs",
    "./websocket-server": "./dist/websocket-server/index.mjs",
    "./server": "./dist/server/index.mjs",
    "./monitoring": "./dist/monitoring/index.mjs",
    "./encryption-key": "./dist/encryption-key/index.mjs",
    "./token": "./dist/token/index.mjs",
    "./merkle-tree": "./dist/merkle-tree/index.mjs",
    "./devtools": "./dist/devtools/index.mjs",
    "./agent": "./dist/agent/index.mjs"
  },
  "main": "./dist/lib/index.mjs",
  "types": "./dist/lib/index.d.mts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "obuild",
    "build:demo": "cd ./playground && bun run build",
    "lint": "eslint . && prettier -c src playground",
    "lint:fix": "automd && eslint . --fix && prettier -w src playground",
    "prepack": "bun run build",
    "dev": "PORT=1235 bun run --watch ./playground/bun/server.ts",
    "dev:encrypted": "PORT=1234 bun run --watch ./playground/bun/encrypted-server.ts",
    "release": "bun run test && changelogen --release && npm publish && git push --follow-tags",
    "test": "bun run test:types && bun test --dots",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@logtape/logtape": "^1.2.0",
    "@tanstack/devtools-event-client": "0.4.0",
    "hookable": "^5.5.3",
    "jose": "^6.1.1",
    "lib0": "^0.2.114",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@nats-io/transport-node": "^3.2.0",
    "@types/bun": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "automd": "^0.4.2",
    "bun-plugin-tailwind": "0.1.2",
    "changelogen": "^0.6.2",
    "crossws": "^0.4.3",
    "eslint": "^9.39.1",
    "eslint-config-unjs": "^0.5.0",
    "eventsource": "^4.1.0",
    "ioredis": "^5.8.2",
    "msw": "2.12.1",
    "obuild": "^0.4.27",
    "prettier": "^3.6.2",
    "srvx": "^0.10.1",
    "typescript": "5.9.3",
    "unstorage": "^1.17.2"
  },
  "peerDependencies": {
    "@nats-io/transport-node": "^3.2.0",
    "crossws": "^0.4.3",
    "ioredis": "^5.8.2",
    "unstorage": "^1.17.2"
  },
  "peerDependenciesMeta": {
    "@nats-io/transport-node": {
      "optional": true
    },
    "crossws": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "unstorage": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "bun@1.3.8"
}
